{
    "functions": {
        "app/api/generate/route.js": {
            "maxDuration": 60
        },
        "app/api/generate-voice/route.js": {
            "maxDuration": 60
        },
        "app/api/profile/update/route.js": {
            "maxDuration": 60
        },
        "app/api/generate-image/route.js": {
            "maxDuration": 60
        }
    },
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, max-age=0"
                }
            ]
        }
    ],
    "regions": ["iad1"],
    "cleanUrls": true,
    "trailingSlash": false
}