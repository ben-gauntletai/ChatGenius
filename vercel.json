{
    "functions": {
        "api/generate.js": {
        "maxDuration": 60
        },
        "api/generate-voice/**": {
        "maxDuration": 60
        },
        "api/profile/update/**": {
        "maxDuration": 60
        },
        "api/generate-image/**": {
        "maxDuration": 60
        }
    },
    "routes": [
        {
        "source": "/api/(.*)",
        "destination": "/api/$1",
        "headers": {
        "Cache-Control": "no-store, max-age=0"
        }
        }
    ]
}